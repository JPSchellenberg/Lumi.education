  "scripts": {
    "start": "npm run dist && npm run dev",
    "clean": "rm -rf dist && rm -rf build",
    "uninstall": "npm run clean && rm -rf node_modules",
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --port 8080 --config './webpack.js' --progress --colors",
    "dist": "npm run clean && npm run build",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack -p --config './webpack.js' --progress --colors",
    "build:server": "./node_modules/.bin/tsc src/server/index.ts --sourceMap --inlineSources --module commonjs --target es5 --outDir build/",
    "build": "npm run build:client && npm run build:server",
    "postinstall": "npm run build"
  },
